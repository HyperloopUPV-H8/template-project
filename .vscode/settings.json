        "**/.cproject": true,
        "**/.gitignore": true,
        "**/.mxproject": true,
        "**/.project": true,
        "**/.settings": true,
        "**/arm-none-eabi.cmake": false,
        "**/build": true,
        "**/cmake_install.cmake": true,
